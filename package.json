{
  "name": "nodeproj",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/index.js",
    "start:docker": "pnpm build && node --inspect=0.0.0.0 dist/index.js",
    "wotcher": "watchy --no-init-spawn --debounce 1 -w src -- docker compose restart backend"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^3.0.2",
    "pug": "2.0.0-beta11",
    "typescript": "^5.3.3",
    "watchy": "^0.10.1"
  },
  "volta": {
    "node": "20.10.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@tsconfig/node20": "^20.1.2",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5"
  }
}
